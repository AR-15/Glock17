   "source": [
    "import pandas as pd\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer, CountVectorizer\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "from sklearn.decomposition import LatentDirichletAllocation, TruncatedSVD\n",
    "import random\n",
    "from matplotlib.pyplot import subplots\n",
    "from seaborn import histplot\n",
    "\n",
    "# 导入必要的库和模块\n",
    "\n",
    "# 读取数据\n",
    "df = pd.read_csv(\"/kaggle/input/imdb-video-games-dataset/imdb_video_games.csv\", index_col=0)\n",
    "# 从CSV文件中读取数据，将第一列作为索引\n",
    "\n",
    "df = df.rename(columns={\"Unnamed: 0\": \"Popularity\"})\n",
    "# 将第一列的列名从\"Unnamed: 0\"更改为\"Popularity\"\n",
    "\n",
    "df = df[pd.to_numeric(df['User Rating'], errors='coerce').notna()]\n",
    "# 从'User Rating'列中提取数值，并删除包含非数值数据的行\n",
    "\n",
    "df['Summary'].fillna('', inplace=True)\n",
    "# 将'Summary'列中的缺失值替换为一个空字符串\n",
    "\n",
    "df['Stars'].fillna('', inplace=True)\n",
    "# 将'Stars'列中的缺失值替换为一个空字符串\n",
    "\n",
    "df['User Rating'] = df['User Rating'].astype(float)\n",
    "# 将'User Rating'列的数据类型更改为浮点数\n",
    "\n",
    "# 数据可视化\n",
    "_, ax = subplots(figsize=(13, 5), ncols=2)\n",
    "# 创建一个包含两个子图的绘图区域\n",
    "\n",
    "histplot(ax=ax[0], data=df, x='User Rating', bins=int(1 + 10 * (df['User Rating'].max() - df['User Rating'].min())), hue='Year', legend=False, palette='Blues')\n",
    "# 绘制用户评分的直方图，分组依据是'Year'列，图例禁用，颜色调色板使用'Blues'\n",
    "\n",
    "histplot(ax=ax[1], data=df, hue='User Rating', bins=70, x='Year', legend=False, palette='Blues')\n",
    "# 绘制用户评分的直方图，分组依据是'User Rating'列，图例禁用，颜色调色板使用'Blues'\n",
    "\n",
    "# TF-IDF和LSA\n",
    "tfidf = TfidfVectorizer(token_pattern=r'[a-zA-Z0-9]+', lowercase=True)\n",
    "# 创建一个TF-IDF向量化器，用于将文本数据转换为TF-IDF权重向量\n",
    "\n",
    "tfidf_matrix = tfidf.fit_transform(df['Summary'])\n",
    "# 将'Summary'列的文本数据转换为TF-IDF矩阵\n",
    "\n",
    "num_topics = 20\n",
    "# 定义主题数量\n",
    "\n",
    "lsa = TruncatedSVD(n_components=num_topics, random_state=42)\n",
    "# 创建一个LSA（Latent Semantic Analysis）模型，将TF-IDF矩阵降维到指定主题数量\n",
    "\n",
    "lsa_matrix = lsa.fit_transform(tfidf_matrix)\n",
    "# 对TF-IDF矩阵进行LSA降维\n",
    "\n",
    "# 随机选择和打印主题中的一些单词\n",
    "num_features = len(tfidf.get_feature_names_out())\n",
    "# 获取TF-IDF向量中的特征数量\n",
    "\n",
    "for _ in range(5):\n",
    "    random_word_index = random.randint(0, num_features - 1)\n",
    "    # 随机选择一个特征的索引\n",
    "\n",
    "    random_word = tfidf.get_feature_names_out()[random_word_index]\n",
    "    # 根据索引获取对应的特征名称\n",
    "\n",
    "    print(\"随机单词:\", random_word)\n",
    "    # 打印随机选择的单词\n",
    "\n",
    "# 显示主题中的顶部单词\n",
    "for index in range(num_topics):\n",
    "    top_word_indices = lsa.components_[index].argsort()[-20:]\n",
    "    # 获取每个主题中权重最高的前20个特征的索引\n",
    "\n",
    "    top_words = [tfidf.get_feature_names_out()[i] for i in top_word_indices]\n",
    "    # 根据索引获取对应的特征名称\n",
    "\n",
    "    print(f'\\n主题 {index} 中的前20个关键词:')\n",
    "    print(top_words)\n",
    "    # 打印每个主题中的前20个关键词\n",
    "\n",
    "# 主题分配\n",
    "df[\"Topic\"] = lsa_matrix.argmax(axis=1)\n",
    "# 为数据框添加一个新的列，其中存储每个文本的主题分配\n",
    "\n",
    "# 使用LDA模型\n",
    "cv = CountVectorizer(max_df=0.95, min_df=2, stop_words='english')\n",
    "# 创建一个计数向量化器，用于构建文档-词频矩阵\n",
    "\n",
    "dtm = cv.fit_transform(df['Summary'])\n",
    "# 将'Summary'列的文本数据转换为文档-词频矩阵\n",
    "\n",
    "LDA = LatentDirichletAllocation(n_components=20, random_state=42)\n",
    "# 创建一个LDA（Latent Dirichlet Allocation）模型，指定主题数量\n",
    "\n",
    "LDA.fit(dtm)\n",
    "# 在文档-词频矩阵上拟合LDA模型\n",
    "\n",
    "# 获取特定主题的单词分布\n",
    "topic_index = 0\n",
    "# 选择一个特定主题的索引\n",
    "\n",
    "topic_word_distribution = LDA.components_[topic_index]\n",
    "# 获取该主题的单词分布\n",
    "\n",
    "top_word_indices = topic_word_distribution.argsort()[-5:]\n",
    "# 获取权重最高的前5个单词的索引\n",
    "\n",
    "top_words = [list(cv.get_feature_names_out())[index] for index in top_word_indices]\n",
    "# 根据索引获取对应的特征名称\n",
    "\n",
    "print(\"主题中的前5个单词:\", top_words)\n",
    "# 打印该主题中的前5个单词\n",
    "\n",
    "# 在LDA模型中迭代每个主题\n",
    "for index, topic in enumerate(LDA.components_):\n",
    "    top_words = [list(cv.get_feature_names_out())[i] for i in topic.argsort()[-20:]]\n",
    "    # 获取每个主题中权重最高的前20个单词\n",
    "\n",
    "    print(f'\\n主题 {index} 中的前20个关键词:')\n",
    "    print(top_words)\n",
    "    # 打印每个主题中的前20个关键词\n",
    "\n",
    "topics = LDA.transform(dtm)\n",
    "# 获取每个文本在各个主题上的分布\n",
    "\n",
    "df[\"Topic\"] = topics.argmax(axis=1)\n",
    "# 为数据框添加一个新的列，其中存储每个文本所属的主题\n",
    "\n",
    "#合并数据\n",
    "df['Combined'] = df['Title'] + ', ' + df['Summary'] + ', ' + df['Genre']\n",
    "# 创建一个新列\"Combined\"，其中包含了\"Title\"、\"Summary\"和\"Genre\"列的合并文本数据\n",
    "\n",
    "# 使用TF-IDF构建余弦相似性矩阵\n",
    "tfidf2 = TfidfVectorizer(token_pattern=r'[a-zA-Z0-9]+', lowercase=True)\n",
    "# 创建另一个TF-IDF向量化器，用于将合并文本数据转换为TF-IDF权重向量\n",
    "\n",
    "tfidf_matrix2 = tfidf2.fit_transform(df['Combined'])\n",
    "# 将\"Combined\"列的文本数据转换为TF-IDF矩阵\n",
    "\n",
    "cosine_sim2 = cosine_similarity(tfidf_matrix2, tfidf_matrix2)\n",
    "# 计算TF-IDF矩阵的余弦相似性矩阵\n",
    "\n",
    "# 游戏推荐函数\n",
    "def recommend(game, num_recommendations=10):\n",
    "    game_index = df[df['Title'] == game].index\n",
    "    # 查找给定游戏的索引\n",
    "\n",
    "    if not game_index.empty:\n",
    "        game_index = game_index[0]\n",
    "    else:\n",
    "        return \"数据集中找不到这款游戏。\"\n",
    "        # 如果游戏不在数据集中，返回错误消息\n",
    "\n",
    "    cosine_scores = list(enumerate(cosine_sim2[game_index]))\n",
    "    # 获取给定游戏与其他游戏的余弦相似性分数\n",
    "\n",
    "    cosine_scores = sorted(cosine_scores, key=lambda x: x[1], reverse=True)\n",
    "    # 根据相似性分数对游戏进行排序\n",
    "\n",
    "    cosine_scores = cosine_scores[1:]\n",
    "    # 去除与自身的相似性分数\n",
    "\n",
    "    top_indices = [x[0] for x in cosine_scores[:num_recommendations]]\n",
    "    # 获取前N个相似游戏的索引\n",
    "\n",
    "    recommended_games = df.iloc[top_indices][['Title', 'Year', 'Genre', 'Certificate', 'User Rating']]\n",
    "    # 获取推荐游戏的相关信息\n",
    "\n",
    "    return recommended_games\n",
    "\n",
    "user_input2 = \"Baldur's Gate III\"\n",
    "# 指定要查找推荐游戏的游戏名称\n",
    "\n",
    "recommendations2 = recommend(user_input2)\n",
    "# 调用游戏推荐函数\n",
    "\n",
    "# It checks if recommendations2 is a string (which would indicate an error message) using isinstance. \n",
    "# If it's a string, it prints the error message directly.\n",
    "if isinstance(recommendations2, str):\n",
    "    print(recommendations2)\n",
    "    # 如果推荐结果是字符串（错误消息），直接打印错误消息\n",
    "else:\n",
    "    print(f\"'{user_input2}' 的推荐游戏:\")\n",
    "    display(recommendations2)\n",
    "    # 否则，打印推荐游戏的相关信息"
   ]
  }
 ],
